*{
    font-size: 12px;
}
.layui-table{
    color: #333333;
    /*font-family: "宋体";*/
}
.table-tit{
    height: 30px;
    line-height: 30px;
    border-bottom: #ddd solid 1px;
    margin-bottom: 5px;
}
.table-tit>a{
    float: right;
    display:inline-block;
    border: #ccc solid 1px;
    height: 16px;
    line-height: 16px;
    margin-top: 5px;
}
.table-tit>a:hover{
    opacity: .7;
}
.table-tit>a i{

}
.layui-elem-field legend{
    font-size: 14px;
}
.layui-table td, .layui-table th{
    padding: 5px 5px;
    height: 22px;
}
.layui-table td, .layui-table th, .layui-table-col-set, .layui-table-fixed-r, .layui-table-grid-down, .layui-table-header, .layui-table-page, .layui-table-tips-main, .layui-table-tool, .layui-table-total, .layui-table-view, .layui-table[lay-skin=line], .layui-table[lay-skin=row]{
    border-color: #000;
}
.style_line_{
    border-bottom: #000 solid 1px;
    padding: 0 10px;
}
.color-tis{
    margin-left: 10px;
    float: left;
    overflow: hidden;
}
.color-tis span{
    float: left;
    margin-right: 10px;
}
.color-tis span>i{
    width: 30px;
    height: 14px;
    display: inline-block;
    float: right;
    margin-top: 3px;
}
.box-blue{
    background-color: #01AAED;
}
.box-green{
    background-color: #5FB878;
}
.box-red{
    border: 1px solid #FF5722;
    color: #FF5722;
    padding: 0 5px;
    display: inline-block;
    font-size: 14px;
}

.box-yellow{
    border: 1px solid #FFB800;
    color: #FFB800;
    padding: 0 5px;
    display: inline-block;
    font-size: 14px;
}
.mr10{
    margin-right: 10px;
}
.input-num{
    width: 24px;
    height: 20px;
    margin-bottom: -7px!important;
    border: #000 solid 1px;
    background-color: #ffffff;
    text-align: center;
    display: inline-block;
}
table.layui-table td em{
    width: 16px;
    height: 16px;
    border-radius: 16px;
    border: #ddd solid 1px;
    display: inline-block;
    text-align: center;
    font-style: normal;
    font-size: 12px;
    margin-right: 3px;
    padding: 0;
}
.layui-table thead.thead_style th{
    font-size: 16px;
    text-align: center;
    background-color: #ffffff;
}

.layui-layer-tips .layui-layer-content{
    background-color: #f2f2f2!important;
    /*border: #ddd solid 1px;*/
}
.layui-layer-tips i.layui-layer-TipsB, .layui-layer-tips i.layui-layer-TipsT{
    border-right-color: #f2f2f2!important;
}
.color-1{
    color: #01aaed;
    /*background: url("../../../static/images/col-1.png") no-repeat 2px center;*/
    border: #01AAED solid 1px;
}
.color-1>em{
    border: #01AAED solid 1px!important;
    margin:2px;
}
.color-2{
    color: #ff5722;
    /*background: url("../../../static/images/col-2.png") no-repeat 2px center;*/
    border: #FF5722 solid 1px;
}
.color-2>b{
    background-color: #ff5722;
    color: #ffffff;
    margin-left: 2px;
    padding-left: 2px;
}
.color-2>em{
    border: #FF5722 solid 1px!important;
    margin:2px;
}
.color-3{
    color: #5fb878;
    /*background: url("../../../static/images/col-3.png") no-repeat 2px center;*/
    border: #5FB878 solid 1px;
}
.color-3>em{
    border: #5fb878 solid 1px!important;
    margin:2px;
}
.color-4{
    color: #ffb800;
    /*background: url("../../../static/images/col-4.png") no-repeat 2px center;*/
    border: #FFB800 solid 1px;
}
.color-4>em{
    border: #ffb800 solid 1px!important;
    margin:2px;
}
tr.layui-bg-gray td{
    color: #cccccc;
}
tr.layui-bg-gray td p:before{
    content: '';
    display: block;
    height: 1px;
    width: 100%;
    background-color: #ccc;
    position: relative;
    top: 10px;/*调节线高*/
    left: 0;
}
td.layui-bg-gray p:before{
    content: '';
    display: block;
    height: 1px;
    width: 100%;
    background-color: #ccc;
    position: relative;
    top: 10px;/*调节线高*/
    left: 0;
}
.layui-table.p_td_0 td{
    padding: 5px 0;
}
.head_page{
    position: absolute;
    top: 5px;
    right: 5px;
    z-index: 1;
    line-height: 28px;
}
.head_page a{

    float: right;
    color: #01aaed;
    margin:0 5px;
}
.head_page input.layui-input{
    float: left;
    width: 40px;
    margin-right: 5px;
}
strong.fw_{
    font-weight: 400;
}

input::-webkit-input-placeholder {
    color: #c0c0c0;
    font-size: 14px;
}
input:-moz-placeholder {
    color: #c0c0c0;
    opacity: 1;
    font-size: 14px;
}
input::-moz-placeholder {
    color: #c0c0c0;
    opacity: 1;
    font-size: 14px;
}
input:-ms-input-placeholder {
    color: #c0c0c0;
    font-size: 14px;
}



.see_last{
    color: #ff0000!important;
    border-bottom: #ff0000 solid 1px;
}
.poa_postil{
    background-color: #ffffff;
    padding: 5px 10px 8px;
    position: absolute;
    top: 0;
    left: -500px;
    z-index: 1;
    border:#bfbfbf solid 1px;
    overflow: auto;
    display: none;
}
.poa_postil.poa_w_all{
    left: 0;
    width: 49%;
    border: none;
    display: block!important;
}
.poa_postil.poa_w_all .fr_close{
    display: none;
}
.poa_postil div>b{
    border-left:#1E9FFF solid 3px ;
    padding-left: 5px;
    font-size: 15px;
    color: #ff0000;
}
.pot_none{
    pointer-events: none;
}
.fr_close{
    float: right;
}

table.layui-table td{word-break: break-all}/*表格英文不换行*/

.fuyue{
    width: 98px;
    height: 98px;
    display: inline-block;
    position: absolute;
    left: -8px;
    top: -8px;
    z-index: 2;
}
.fuyue.i_yes{
    background: url("../images/fy01.png") no-repeat top left;
}
.fuyue.i_no{
    background: url("../images/fy02.png") no-repeat top left;
}

.dafen{
    width: 98px;
    height: 98px;
    display: inline-block;
    position: absolute;
    right: 0px;
    top: -8px;
    z-index: 1;
    background: url("../images/df01.png") no-repeat top left;
}
.dafen>span{
    font-size: 30px;
    font-weight: bold;
    color: #ffffff;
    text-align: center;
    float: right;
    margin-top: 12px;
    margin-right: 1px;
    text-shadow: 3px 2px 5px #333;
    transform: rotate(45deg);
    -ms-transform: rotate(45deg);
    -moz-transform: rotate(45deg);
    -webkit-transform: rotate(45deg);
    -o-transform: rotate(45deg);
    width: 66px;
}

#bing-body>div>form{
    width: 100%;
}